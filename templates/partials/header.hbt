<!doctype html>
<html lang="en" class="color--{{this.pageColor}} no-js no-touch" data-page-color-original="{{this.pageColor}}">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>{{title}} &middot; {{siteInfo 'title'}}</title>
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <meta property="og:url"         content="{{siteInfo 'url'}}">
  <meta property="og:type"        content="website">
  <meta property="og:title"       content="{{siteInfo 'author'}}">
  <meta property="og:description" content="{{siteInfo 'description'}}">
  <meta property="og:image"       content="{{siteInfo 'url'}}/assets/images/screenshot.png">
  <meta name="twitter:card"       content="summary">
  <meta name="twitter:site"       content="@{{siteInfo 'twitter'}}">
  <meta name="twitter:creator"    content="@{{siteInfo 'twitter'}}">

  <link rel="stylesheet" href="{{siteInfo 'url'}}/assets/css/style.css" media="screen">
  <link rel="stylesheet" href="{{siteInfo 'url'}}/assets/css/print.css" media="print">

  <!-- Fav- & Apple-Touch-Icons -->
  <!-- Favicon -->
  <link rel="shortcut icon" href="{{siteInfo 'url'}}/assets/images/favicon/favicon.ico" type="image/x-icon"/>
  <!-- non-retina iPhone pre iOS 7 -->
  <link rel="apple-touch-icon" href="{{siteInfo 'url'}}/assets/images/favicon/114x114.png" sizes="57x57">
  <!-- non-retina iPad pre iOS 7 -->
  <link rel="apple-touch-icon" href="{{siteInfo 'url'}}/assets/images/favicon/144x144.png" sizes="72x72">
  <!-- non-retina iPad iOS 7 -->
  <link rel="apple-touch-icon" href="{{siteInfo 'url'}}/assets/images/favicon/152x152.png" sizes="76x76">
  <!-- retina iPhone pre iOS 7 -->
  <link rel="apple-touch-icon" href="{{siteInfo 'url'}}/assets/images/favicon/114x114.png" sizes="114x114">
  <!-- retina iPhone iOS 7 -->
  <link rel="apple-touch-icon" href="{{siteInfo 'url'}}/assets/images/favicon/120x120.png" sizes="120x120">
  <!-- retina iPad pre iOS 7 -->
  <link rel="apple-touch-icon" href="{{siteInfo 'url'}}/assets/images/favicon/144x144.png" sizes="144x144">
  <!-- retina iPad iOS 7 -->
  <link rel="apple-touch-icon" href="{{siteInfo 'url'}}/assets/images/favicon/152x152.png" sizes="152x152">
  <!-- retina iPhone 6 iOS 7 -->
  <link rel="apple-touch-icon" href="{{siteInfo 'url'}}/assets/images/favicon/180x180.png" sizes="180x180">

